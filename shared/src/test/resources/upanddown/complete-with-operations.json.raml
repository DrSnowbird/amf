#%RAML 1.0
title: test title
description: test description
mediaType:
  - application/yaml
version: 1.1
(termsOfService): terms of service
protocols:
  - http
  - https
(contact):
  url: contactUrl
  name: contactName
  email: contactEmail
(externalDocs):
  url: externalDocsUrl
  description: externalDocsDescription
(license):
  url: licenseUrl
  name: licenseName
