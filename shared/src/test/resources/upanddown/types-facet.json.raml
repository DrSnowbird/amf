#%RAML 1.0
title: test title
types:
  ScalarTest:
    enum:
      - a1
      - a2
    type: string
    pattern: blabla.+
    minLength: 1
    maxLength: 20
  IntegerTest:
    type: integer
    minimum: 2
    maximum: 25
    (exclusiveMinimum): 4
    (exclusiveMaximum): 20
    multipleOf: 2
    format: int32
  ObjectTest:
    displayName: that person
    description: ooo person
    default: defaultValue
    (externalDocs):
      url: externalDocsUrl
      description: externalDocsDescription
    xml:
      attribute: true
      wrapped: true
      name: test
      namespace: namespacetest
      prefix: blabla
    minProperties: 1
    maxProperties: 5
    additionalProperties: true
    discriminator: ddd
    discriminatorValue: valueD
    (readOnly): false
    properties:
      name:
        type: string