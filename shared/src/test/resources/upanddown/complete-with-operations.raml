#%RAML 1.0
title: test title
description: test description
(termsOfService): terms of service
version: 1.1
(license):
  url: licenseUrl
  name: licenseName
mediaType:
  - application/yaml
protocols:
  - http
  - https
(contact):
  url: contactUrl
  name: contactName
  email: contactEmail
documentation:
  -
    content: externalDocsDescription
    (url): externalDocsUrl
/levelzero:
  displayName: Name
  get:
    displayName: Some title
    description: Some description
    (deprecated): true
    (summary): This is a summary
    (externalDocs):
      url: urlExternalDocs
      description: descriptionExternalDocs
    protocols:
      - http
      - https
    (consumes):
      - application/xml
    (produces):
      - application/json
  post:
    displayName: Some title
    description: Some description
    (externalDocs):
      url: urlExternalDocs
      description: descriptionExternalDocs
    protocols:
      - http
      - https
  /level-one:
    displayName: One display name
    description: and this description!
    get:
      displayName: Some title level one
      description: Some description level one
      (deprecated): true
      (summary): This is a summary level one
      (externalDocs):
        url: urlExternalDocs lvel one
        description: descriptionExternalDocs level one
      protocols:
        - http
        - https
  /another-level-one:
    displayName: some other display name
/another-levelzero:
  displayName: Root name
  post:
    displayName: Some title another level 0
    description: Some description another level 0
    (externalDocs):
      url: urlExternalDocs another level 0
      description: descriptionExternalDocs another level 0
    protocols:
      - http
      - https
baseUri: api.example.com/path
