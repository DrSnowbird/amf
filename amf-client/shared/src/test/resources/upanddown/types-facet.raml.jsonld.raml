#%RAML 1.0
types:
  ScalarTest:
    type: string
    enum:
      - a1
      - a2
    pattern: blabla.+
    minLength: 1
    default: defaultValue
    maxLength: 20
  IntegerTest:
    type: number
    minimum: 2
    maximum: 25
    (exclusiveMinimum): 4
    (exclusiveMaximum): 20
    format: int32
    multipleOf: 2
  ObjectTest:
    displayName: that person
    description: ooo person
    default:
      name: john doe
    (externalDocs):
      url: externalDocsUrl
      description: externalDocsDescription
    properties:
      name:
        type: string
        (readOnly): false
    minProperties: 1
    maxProperties: 5
    discriminator: ddd
    discriminatorValue: valueD
    xml:
      namespace: namespacetest
      prefix: blabla
title: test title
