#%RAML 1.0
title: Slideshare
version: 2
protocols:
  - https
baseUri: www.slideshare.net/api/{version}
/get_slideshow:
  displayName: get_slideshow
  get:
    description: Returns slideshow object
    queryParameters:
      username:
        description: username of requesting user
        required: false
      password:
        description: password of requesting user
        required: false
      detailed:
        description: Set it to 1 if you want a detailed response
        type: integer
        minimum: 0
        maximum: 1
      slideshow_url:
        description: Url of the slideshow being queried for. One of Id or URL is requried
      exclude_tags:
        description: Set it to true if you want exclude tags in the detailed response
        type: boolean
      get_transcript:
        description: Set it to 1 if you want to return with transcript parameter in the response. requires detailed to be set to 1.
        type: integer
        minimum: 0
        maximum: 1
      slideshow_id:
        type: integer
        description: Id of the slideshow being queried for. One of Id or URL is required
        example: 13343768
    responses:
      200:
        body:
          application/xml: "<xs:schema attributeFormDefault=\"unqualified\" elementFormDefault=\"qualified\" xmlns:xs=\"http://www.w3.org/2001/XMLSchema\">\r\n  <xs:element name=\"slideshow\">\r\n    <xs:complexType mixed=\"true\">\r\n      <xs:sequence>\r\n        <xs:element type=\"xs:string\" name=\"id\"/>\r\n        <xs:element type=\"xs:string\" name=\"title\"/>\r\n        <xs:element type=\"xs:string\" name=\"description\"/>\r\n        <xs:element type=\"xs:string\" name=\"status\"/>\r\n        <xs:element type=\"xs:string\" name=\"username\"/>\r\n        <xs:element type=\"xs:string\" name=\"url\"/>\r\n        <xs:element type=\"xs:string\" name=\"thumbnailurl\"/>\r\n        <xs:element type=\"xs:string\" name=\"thumbnailsmallurl\"/>\r\n        <xs:element type=\"xs:string\" name=\"embed\"/>\r\n        <xs:element type=\"xs:string\" name=\"created\"/>\r\n        <xs:element type=\"xs:string\" name=\"updated\"/>\r\n        <xs:element type=\"xs:string\" name=\"language\"/>\r\n        <xs:element type=\"xs:string\" name=\"format\"/>\r\n        <xs:element type=\"xs:string\" name=\"download\"/>\r\n        <xs:element type=\"xs:string\" name=\"downloadurl\"/>\r\n        <xs:element type=\"xs:string\" name=\"slideshowtype\"/>\r\n        <xs:element name=\"incontest\">\r\n          <xs:complexType mixed=\"true\">\r\n            <xs:sequence>\r\n              <xs:element type=\"xs:string\" name=\"userid\"/>\r\n              <xs:element type=\"xs:string\" name=\"externalappuserid\"/>\r\n              <xs:element type=\"xs:string\" name=\"externalappid\"/>\r\n              <xs:element name=\"pptlocation\">\r\n                <xs:complexType mixed=\"true\">\r\n                  <xs:sequence>\r\n                    <xs:element type=\"xs:string\" name=\"strippedtitle\"/>\r\n                    <xs:element name=\"tags\">\r\n                      <xs:complexType>\r\n                        <xs:sequence>\r\n                          <xs:element name=\"tag\">\r\n                            <xs:complexType>\r\n                              <xs:simpleContent>\r\n                                <xs:extension base=\"xs:string\">\r\n                                  <xs:attribute type=\"xs:string\" name=\"count\"/>\r\n                                  <xs:attribute type=\"xs:string\" name=\"owner\"/>\r\n                                </xs:extension>\r\n                              </xs:simpleContent>\r\n                            </xs:complexType>\r\n                          </xs:element>\r\n                        </xs:sequence>\r\n                      </xs:complexType>\r\n                    </xs:element>\r\n                    <xs:element type=\"xs:string\" name=\"audio\"/>\r\n                    <xs:element type=\"xs:string\" name=\"numdownloads\"/>\r\n                    <xs:element type=\"xs:string\" name=\"numviews\"/>\r\n                    <xs:element type=\"xs:string\" name=\"numcomments\"/>\r\n                    <xs:element type=\"xs:string\" name=\"numfavorites\"/>\r\n                    <xs:element type=\"xs:string\" name=\"numslides\"/>\r\n                    <xs:element name=\"relatedslideshows\">\r\n                      <xs:complexType>\r\n                        <xs:sequence>\r\n                          <xs:element name=\"relatedslideshowid\">\r\n                            <xs:complexType>\r\n                              <xs:simpleContent>\r\n                                <xs:extension base=\"xs:string\">\r\n                                  <xs:attribute type=\"xs:string\" name=\"rank\"/>\r\n                                </xs:extension>\r\n                              </xs:simpleContent>\r\n                            </xs:complexType>\r\n                          </xs:element>\r\n                        </xs:sequence>\r\n                      </xs:complexType>\r\n                    </xs:element>\r\n                    <xs:element type=\"xs:string\" name=\"privacylevel\"/>\r\n                    <xs:element type=\"xs:string\" name=\"flagvisible\"/>\r\n                    <xs:element type=\"xs:string\" name=\"showonss\"/>\r\n                    <xs:element type=\"xs:string\" name=\"secreturl\"/>\r\n                    <xs:element type=\"xs:string\" name=\"allowembed\"/>\r\n                    <xs:element type=\"xs:string\" name=\"sharewithcontacts\"/>\r\n                  </xs:sequence>\r\n                </xs:complexType>\r\n              </xs:element>\r\n            </xs:sequence>\r\n          </xs:complexType>\r\n        </xs:element>\r\n      </xs:sequence>\r\n    </xs:complexType>\r\n  </xs:element>\r\n</xs:schema>\r\n"
    securedBy: []
/get_slideshows_by_tag:
  displayName: get_slideshows_by_tag
  get:
    description: Returns slideshows that contain the specified tag
    queryParameters:
      limit:
        description: Specify the number of slideshows to return
        example: 10
        type: integer
        minimum: 1
      offset:
        description: specify offset
        type: integer
        minimum: 0
      detailed:
        description: Set it to 1 if you want a detailed response
        type: integer
        minimum: 0
        maximum: 1
      tag:
        description: Tag name
        required: true
        example: slideshare
    responses:
      200:
        body:
          application/xml: "<xs:schema attributeFormDefault=\"unqualified\" elementFormDefault=\"qualified\" xmlns:xs=\"http://www.w3.org/2001/XMLSchema\">\r\n  <xs:element name=\"tag\">\r\n    <xs:complexType>\r\n      <xs:sequence>\r\n        <xs:element type=\"xs:string\" name=\"name\"/>\r\n        <xs:element type=\"xs:string\" name=\"count\"/>\r\n        <xs:element name=\"slideshow\" maxOccurs=\"unbounded\" minOccurs=\"0\">\r\n          <xs:complexType mixed=\"true\">\r\n            <xs:sequence>\r\n              <xs:element type=\"xs:string\" name=\"id\"/>\r\n              <xs:element type=\"xs:string\" name=\"title\"/>\r\n              <xs:element type=\"xs:string\" name=\"description\"/>\r\n              <xs:element type=\"xs:string\" name=\"status\"/>\r\n              <xs:element type=\"xs:string\" name=\"username\"/>\r\n              <xs:element type=\"xs:string\" name=\"url\"/>\r\n              <xs:element type=\"xs:string\" name=\"thumbnailurl\"/>\r\n              <xs:element type=\"xs:string\" name=\"thumbnailsmallurl\"/>\r\n              <xs:element type=\"xs:string\" name=\"embed\"/>\r\n              <xs:element type=\"xs:string\" name=\"created\"/>\r\n              <xs:element type=\"xs:string\" name=\"updated\"/>\r\n              <xs:element type=\"xs:string\" name=\"language\"/>\r\n              <xs:element type=\"xs:string\" name=\"format\"/>\r\n              <xs:element type=\"xs:string\" name=\"download\"/>\r\n              <xs:element type=\"xs:string\" name=\"downloadurl\"/>\r\n              <xs:element type=\"xs:string\" name=\"slideshowtype\"/>\r\n              <xs:element name=\"incontest\">\r\n                <xs:complexType mixed=\"true\">\r\n                  <xs:sequence>\r\n                    <xs:element type=\"xs:string\" name=\"userid\"/>\r\n                    <xs:element type=\"xs:string\" name=\"externalappuserid\"/>\r\n                    <xs:element type=\"xs:string\" name=\"externalappid\"/>\r\n                    <xs:element name=\"pptlocation\">\r\n                      <xs:complexType mixed=\"true\">\r\n                        <xs:sequence>\r\n                          <xs:element type=\"xs:string\" name=\"strippedtitle\"/>\r\n                          <xs:element name=\"tags\">\r\n                            <xs:complexType>\r\n                              <xs:sequence>\r\n                                <xs:element name=\"tag\">\r\n                                  <xs:complexType>\r\n                                    <xs:simpleContent>\r\n                                      <xs:extension base=\"xs:string\">\r\n                                        <xs:attribute type=\"xs:string\" name=\"count\" use=\"optional\"/>\r\n                                        <xs:attribute type=\"xs:string\" name=\"owner\" use=\"optional\"/>\r\n                                      </xs:extension>\r\n                                    </xs:simpleContent>\r\n                                  </xs:complexType>\r\n                                </xs:element>\r\n                              </xs:sequence>\r\n                            </xs:complexType>\r\n                          </xs:element>\r\n                          <xs:element type=\"xs:string\" name=\"audio\"/>\r\n                          <xs:element type=\"xs:string\" name=\"numdownloads\"/>\r\n                          <xs:element type=\"xs:string\" name=\"numviews\"/>\r\n                          <xs:element type=\"xs:string\" name=\"numcomments\"/>\r\n                          <xs:element type=\"xs:string\" name=\"numfavorites\"/>\r\n                          <xs:element type=\"xs:string\" name=\"numslides\"/>\r\n                          <xs:element name=\"relatedslideshows\">\r\n                            <xs:complexType>\r\n                              <xs:sequence>\r\n                                <xs:element name=\"relatedslideshowid\">\r\n                                  <xs:complexType>\r\n                                    <xs:simpleContent>\r\n                                      <xs:extension base=\"xs:string\">\r\n                                        <xs:attribute type=\"xs:string\" name=\"rank\" use=\"optional\"/>\r\n                                      </xs:extension>\r\n                                    </xs:simpleContent>\r\n                                  </xs:complexType>\r\n                                </xs:element>\r\n                              </xs:sequence>\r\n                            </xs:complexType>\r\n                          </xs:element>\r\n                          <xs:element type=\"xs:string\" name=\"privacylevel\"/>\r\n                          <xs:element type=\"xs:string\" name=\"flagvisible\"/>\r\n                          <xs:element type=\"xs:string\" name=\"showonss\"/>\r\n                          <xs:element type=\"xs:string\" name=\"secreturl\"/>\r\n                          <xs:element type=\"xs:string\" name=\"allowembed\"/>\r\n                          <xs:element type=\"xs:string\" name=\"sharewithcontacts\"/>\r\n                        </xs:sequence>\r\n                      </xs:complexType>\r\n                    </xs:element>\r\n                  </xs:sequence>\r\n                </xs:complexType>\r\n              </xs:element>\r\n            </xs:sequence>\r\n          </xs:complexType>\r\n        </xs:element>\r\n      </xs:sequence>\r\n    </xs:complexType>\r\n  </xs:element>\r\n</xs:schema>\r\n"
    securedBy: []
