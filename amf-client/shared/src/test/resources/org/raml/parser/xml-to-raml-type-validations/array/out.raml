#%RAML 1.0
title: simple object
types:
  root:
    properties:
      name:
        type: string
      friends: Friend[]
    additionalProperties: false
  Friend:
    properties:
      name:
        type: string
    additionalProperties: true
    type: object
/res:
  get:
    body:
      application/xml:
        type: root
        example: <root> <name>robert</name> <Friend> <name>daenerys</name> </Friend> <Friend> <name>stannis</name> </Friend> <Friend> <name>jon</name> <night-watch>true</night-watch> </Friend> </root>
