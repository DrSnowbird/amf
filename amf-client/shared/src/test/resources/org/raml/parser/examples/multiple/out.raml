#%RAML 1.0
title: API with Examples
types:
  User:
    properties:
      name:
        type: string
      lastname:
        type: string
    example:
      name: Bob
      lastname: Marley
  Org:
    properties:
      name:
        type: string
      address?:
        type: string
      value?:
        type: string
      age?:
        type: number
/organisation:
  post:
    headers:
      UserID:
        description: the identifier for the user that posts a new organisation
        type: string
        example: SWED-123
    body:
      application/json:
        type: Org
        example: Silver
  get:
    description: Returns an organisation entity.
    responses:
      201:
        body:
          application/json:
            type: Org
            examples:
              acme:
                name: Acme
              softwareCorp: Gold
