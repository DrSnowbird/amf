{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://schemas.asciano.com.au/json/v1/rosterchanges",
  "title": "Roster changes",
  "description": "Collection of roster changes",  
  "type": "array",
  "additionalItems": false,
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      },
      "rosterChangeType": {
        "type": "string"
      },
      "rosterChangeTypeDesc": {
        "type": "string"
      },
      "dateAppliedFor": {
        "type": "string",
        "format": "date"
      },
      "status": {
        "type": "string"
      }
    },
    "additionalProperties": false,
    "required": ["id"]
  }
}