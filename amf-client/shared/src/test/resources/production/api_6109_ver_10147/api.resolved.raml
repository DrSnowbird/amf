                  "@id": /lot/12345
              firstPage: /lot?page=1
              lastPage: /lot?page=1
              previousPage: null
              nextPage: null
            type: |
              {
                "$schema": "http://json-schema.org/draft-04/schema",
                "allOf": [{"$ref": "/schema/pagedCollection"}],
                "properties": {
                  "member": {
                    "type": "array","additionalItems": {"$ref": "/schema/lot"}
