/client:
  get:
    description: |
      Returns (a page from) the collection of all available clients.
      "Paged" means only a subset (one page) is returned at a time.
    responses:
      200:
        body:
          application/json:
            example:
              "@id": /client
              totalItems: 1
              itemsPerPage: 10
              itemOffset: 0
              member:
                -
                  "@id": /client/45678
              firstPage: /client?page=1
              lastPage: /client?page=1
              previousPage: null
              nextPage: null
            type: |
              {
                "$schema": "http://json-schema.org/draft-04/schema",
                "allOf": [{"$ref": "/schema/pagedCollection"}],
                "properties": {
                  "member": {
                    "type": "array","additionalItems": {"$ref": "/schema/client"}
                  }
                }
              }
