                        type: string
                        pattern: -?\d{1,3}\.\d{1,8}
                      Longitude:
                        description: The longitude measured in decimal format according to ISO 213
                        type: string
                        pattern: -?\d{1,3}\.\d{1,8}
                  OpeningTimes:
