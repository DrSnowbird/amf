/workorders:
  get:
    displayName: "Get Work Orders"
    description: "Returns a list of work orders."
    queryParameters:
      list:
        type: "string"
        description: "Saved group which sandboxes filters, sorts and column preferences\
          \ in addition to pre-applied settings (e.g.: tabs or saved searches)"
        required: false
      columns:
        type: "string"
        description: "Specify the columns and their order to return data for"
        required: false
      page:
        type: "integer"
        description: "Specify the page number"
        required: false
      per_page:
        type: "integer"
        description: "Specify the results per page"
        required: false
      view:
        type: "string"
        description: "Caters the response data to how you're going to display it.\
          \ For most uses, you'll want 'list'; other options might include map, schedule,\
          \ tiles, etc."
        required: false
      sticky:
        type: "boolean"
        description: "Persists filters, columns, and sorts (all but pagination) to\
          \ re-apply on future requests where omitted. Persists to the 'list' specified\
          \ for the authenticated user"
        default: false
        required: false
      sort:
        type: "string"
        description: "Sorts results by a specified column id"
        required: false
      order:
        type: "string"
        description: "Whether the sort should be ascending or descending"
        enum:
        - "asc"
        - "desc"
        required: false
      f_:
        type: "string"
        description: "Clears all filters"
        required: false
      f_max_approval_time:
        type: "integer"
        description: "(Provider) Filters work to where company has specified their\
          \ maximum approval day target is less than or equal to provided number of\
          \ days"
        required: false
      f_rating:
        type: "string"
        description: "(Provider) Filters work by buyer's star rating (Buyer) Filters\
          \ work by assigned provider's star rating -- value can be a single number\
          \ (e.g.: 3.5) where that value must be met or exceededor a specific inclusive\
          \ range (e.g.: 3.5,5.0)"
        required: false
      f_flightboard_tomorrow:
        type: "boolean"
        description: "(Provider) Used by the confirm tomorrow work screen"
        required: false
      f_requests:
        type: "boolean"
        description: "(Buyer) Filters work with requests (e.g.: true) or with no requests\
          \ (e.g.: false)"
        required: false
      f_counter_offers:
        type: "boolean"
        description: "(Buyer) Filters work with counter-offer requests of any type\
          \ (schedule, expense or pay change) when true (e.g.: true) or with no counter\
          \ offers (e.g.: false)"
        required: false
      f_work_order_id:
        type: "string"
        description: "(Buyer/Provider) Filters work by specific work order ids"
        required: false
      f_hourly:
        type: "string"
        description: "(Buyer/Provider) Filters work by that of a minimum hourly rate\
          \ (includes only hourly or blended pay struture) -- may be given a single\
          \ value which must be met or exceeded (e.g.: 50) or an inclusive range (e.g.:\
          \ 35,75)"
        required: false
      f_fixed:
        type: "string"
        description: "(Buyer/Provider) Filters work by that of a minimum fixed rate\
          \ (includes only fixed or blended pay structure) -- may by given a single\
          \ value which must be met or exceeded (e.g.: 250) or an inclusive range\
          \ (e.g.: 250,500)"
        required: false
      f_device:
        type: "string"
        description: "(Buyer/Provider) Filters work by that of a minimum per-device\
          \ rate (includes only device pay structure) -- may by given a single value\
          \ which must be met or exceeded (e.g.: 250) or an inclusive range (e.g.:\
          \ 250,500)"
        required: false
      f_pay:
        type: "string"
        description: "(Buyer/Provider) Filters work by that whose minimum or maximum\
          \ payout meets or exceeds a single value (e.g.: 250) or whose minimum or\
          \ maximum payout falls between an inclusive range (e.g.: 250,500)"
        required: false
      f_templates:
        type: "string"
        description: "(Buyer) Filters work orders created using a given template,\
          \ provided either by name or it's unique id. May be given as a single value\
          \ or as a list of comma-separated values. If the template name itself has\
          \ a comma, you should use its unique identifier instead"
        required: false
      f_type_of_work:
        type: "string"
        description: "(Buyer/Provider) Filters work by that of a given type (e.g.:\
          \ Networking). May be given a single or comma separated list of values of\
          \ the full descriptions (Networking) or their unique identifiers (40) --\
          \ if the descriptions include commas you should use their unique identifiers"
        required: false
      f_time_zone:
        type: "string"
        description: "(Buyer/Provider) Filters work by that of a given time zone (e.g.:\
          \ America/New_York), list of time zones (e.g.: America/New_York,America/Chicago),\
          \ its offset (e.g.: -5) or a range of offsets (e.g.: -4,-6)"
        required: false
      f_mode:
        type: "string"
        description: "(Buyer/Provider) Filters work by that of a given mode which\
          \ must be one of the following values: 'managed', 'marketplace', or 'any'"
        enum:
        - "managed"
        - "marketplace"
        - "any"
        required: false
      f_company:
        type: "string"
        description: "(Buyer/Provider) Filters work by buyer, can be given text which\
          \ would free-form search all buyers by name (e.g.: 'acme'), or a company's\
          \ unique identifier (e.g.: '50'). You can also include a list of unique\
          \ identifiers (e.g.: 50,51,52) to search for multiple buyers"
        required: false
      f_worked_with:
        type: "string"
        description: "(Provider) Filters work orders to include only those by company's\
          \ I or my vendor/service company has previously worked with"
        required: false
      f_manager:
        type: "string"
        description: "(Buyer) Filters work to that assigned to a given company user,\
          \ either by name (case insensitive), email or their unique id or a list\
          \ of comma separated values of any of these"
        required: false
      f_client:
        type: "string"
        description: "(Buyer) Filters work to that assigned to a given client company,\
          \ either by name (case insensitive), or their unique id or a list of comma\
          \ separated values of either of these"
        required: false
      f_project:
        type: "string"
        description: "(Buyer) Filters work to that assigned to a given project, either\
          \ by name (case insensitive), or their unique id or a list of comma separated\
          \ values of either of these"
        required: false
      f_approval_window:
        type: "string"
        description: "(Provider) Filters work to include only companies who set their\
          \ target approval days equal or less than provided value (e.g.: 2) or within\
          \ a given range (e.g.: 1,3)"
        required: false
      f_review_window:
        type: "string"
        description: "(Buyer) Filters work to include only work orders that exceed\
          \ the days given, or the company's target approval days when given no value"
        required: false
      f_network:
        type: "string"
        description: "(Buyer) Filters work by one or more of your company's networks.\
          \ You must have the Field Nation Complete feature to enable new networks.\
          \ Networks may be provided as a comma separated list of their names (e.g.:\
          \ Field Nation) or unique ids"
        required: false
      f_auto_assign:
        type: "string"
        description: "(Buyer) Filters work to that which was auto-assigned/dispatched\
          \ (when true) or to that which was not (when false)"
        required: false
      f_schedule:
        type: "string"
        description: "(Buyer/Provider) Filters work by a given service date, including\
          \ the whole day when provided a single date (e.g.: 2016-01-01), or between\
          \ two dates (inclusive) when given a range (e.g.: 2016-01-01,2016-01-31).\
          \ Incoming value is assumed to be UTC and will be converted to the work\
          \ order's local time when comparing"
        required: false
      f_created:
        type: "string"
        description: "(Buyer) Filters work created on a date, including the whole\
          \ day when provided a single date (e.g.: 2016-01-01), or between two dates\
          \ (inclusive) when given a range (e.g.: 2016-01-01,2016-01-31). Incoming\
          \ value is assumed to be UTC and will be converted to the work order's local\
          \ time when comparing"
        required: false
      f_published:
        type: "string"
        description: "(Buyer/Provider) Filters work published on a date, including\
          \ the whole day when provided a single date (e.g.: 2016-01-01), or between\
          \ two dates (inclusive) when given a range (e.g.: 2016-01-01,2016-01-31).\
          \ Incoming value is assumed to be UTC and will be converted to the work\
          \ order's local time when comparing"
        required: false
      f_routed:
        type: "string"
        description: "(Buyer/Provider) Filters work routed on a date, including the\
          \ whole day when provided a single date (e.g.: 2016-01-01), or between two\
          \ dates (inclusive) when given a range (e.g.: 2016-01-01,2016-01-31). Incoming\
          \ value is assumed to be UTC and will be converted to the work order's local\
          \ time when comparing"
        required: false
      f_published_routed:
        type: "string"
        description: "(Buyer/Provider) Filters work published or routed on a date,\
          \ including the whole day when provided a single date (e.g.: 2016-01-01),\
          \ or between two dates (inclusive) when given a range (e.g.: 2016-01-01,2016-01-31).\
          \ Incoming value is assumed to be UTC and will be converted to the work\
          \ order's local time when comparing"
        required: false
      f_completed:
        type: "string"
        description: "(Buyer/Provider) Filters work marked complete on a date, including\
          \ the whole day when provided a single date (e.g.: 2016-01-01), or between\
          \ two dates (inclusive) when given a range (e.g.: 2016-01-01,2016-01-31).\
          \ Incoming value is assumed to be UTC and will be converted to the work\
          \ order's local time when comparing"
        required: false
      f_approved_cancelled:
        type: "string"
        description: "(Buyer/Provider) Filters work approved or cancelled on a date,\
          \ including the whole day when provided a single date (e.g.: 2016-01-01),\
          \ or between two dates (inclusive) when given a range (e.g.: 2016-01-01,2016-01-31).\
          \ Incoming value is assumed to be UTC and will be converted to the work\
          \ order's local time when comparing"
        required: false
      f_confirmed:
        type: "string"
        description: "(Buyer/Provider) Filters work confirmed on a date, including\
          \ the whole day when provided a single date (e.g.: 2016-01-01), or between\
          \ two dates (inclusive) when given a range (e.g.: 2016-01-01,2016-01-31).\
          \ Incoming value is assumed to be UTC and will be converted to the work\
          \ order's local time when comparing"
        required: false
      f_assigned:
        type: "string"
        description: "(Buyer/Provider) Filters work assigned on a date, including\
          \ the whole day when provided a single date (e.g.: 2016-01-01), or between\
          \ two dates (inclusive) when given a range (e.g.: 2016-01-01,2016-01-31).\
          \ Incoming value is assumed to be UTC and will be converted to the work\
          \ order's local time when comparing"
        required: false
      f_saved_location:
        type: "string"
        description: "(Buyer) Filters work to that assigned to a saved location by\
          \ name (e.g.: Acme store #1) or its unique identifier. Can also be given\
          \ a list of comma separated values of either type. If the name includes\
          \ a comma, you should use a list of the unique identifiers"
        required: false
      f_saved_location_group:
        type: "string"
        description: "(Buyer) Filters work to that assigned to a saved location group\
          \ by name (e.g.: Acme stores) or its unique identifier. Can also be given\
          \ a list of comma separated values of either type. If the name includes\
          \ a comma, you should use a list of the unique identifiers"
        required: false
      f_city:
        type: "string"
        description: "(Buyer/Provider) Filters work to include only that of a given\
          \ city name (case insensitive) or a list of cities when given a comma separated\
          \ list of values"
        required: false
      f_state:
        type: "string"
        description: "(Buyer/Provider) Filters work to include only that of a given\
          \ state (case insensitive) or a list of states when given a comma separated\
          \ list of values"
        required: false
      f_postal_code:
        type: "string"
        description: "(Buyer/Provider) Filters work to include only that of a given\
          \ postal code (case insensitive) or a list of postal codes when given a\
          \ comma separated list of values"
        required: false
      f_country:
        type: "string"
        description: "(Buyer/Provider) Filters work to include only that of a given\
          \ country (case insensitive) or a list of countries when given a comma separated\
          \ list of values"
        required: false
      f_flags:
        type: "string"
