#%RAML 1.0
title: "Fieldnation RESTful API V2"
version: "2.0.0"
protocols:
- "HTTPS"
baseUri: "https://api.fieldnation.com/api/rest/v2"
mediaType:
- "application/json"
types:
  AttachmentConflict:
    type: "object"
    properties:
      status:
        type: "string"
        enum:
        - "unique"
        - "duplicate"
        required: false
      work_order_id:
        type: "integer"
        required: false
      conflicts:
        type: "array"
        required: false
        items:
          type: "integer"
  Assignee:
    type: "object"
    properties:
      correlation_id:
        type: "string"
        required: false
      role:
        type: "string"
        required: false
      user:
        type: "User"
        required: false
      work_order_id:
        type: "integer"
        required: false
      status_id:
        type: "integer"
        required: false
      actions:
        type: "array"
        required: false
        items:
          type: "string"
          enum:
          - "unassign"
          - "ready_to_go"
      reason_id:
        type: "integer"
        required: false
      comment:
        type: "string"
        required: false
  AutoDispatch:
    type: "object"
    properties:
      work_order_ids:
        type: "array"
        required: false
        items:
          type: "integer"
      selection_rule_id:
        type: "integer"
        required: false
      delay:
        type: "integer"
        required: false
      w2_skippub:
        type: "boolean"
        required: false
      w2_failswitch:
        type: "boolean"
        required: false
      should_route:
        type: "boolean"
        required: false
  CompanyRating:
    type: "object"
    properties:
      rating:
        type: "object"
        required: false
        properties:
          marketplace:
            type: "Satisfaction"
            required: false
          mine:
            type: "Satisfaction"
            required: false
          toggle_visible:
            type: "boolean"
            required: false
  BackgroundCheck:
    type: "object"
    properties:
      id:
        type: "integer"
        required: false
      name:
        type: "string"
        required: false
      updated:
        type: "string"
        required: false
      expires:
        type: "string"
        required: false
      highlights:
        type: "array"
        required: false
        items:
          type: "string"
  Banner:
    type: "array"
    items:
      type: "object"
      properties:
        id:
          type: "integer"
          required: false
        user_id:
          type: "integer"
          required: false
        image:
          type: "string"
          required: false
        href:
          type: "string"
          required: false
        deleted:
          type: "boolean"
          required: false
        author:
          type: "User"
          required: false
        groups:
          type: "array"
          required: false
          items:
            type: "integer"
        company_ids:
          type: "array"
          required: false
          items:
            type: "integer"
        created_at:
          type: "Utc"
          required: false
        starts_at:
          type: "Utc"
          required: false
        ends_at:
          type: "Utc"
          required: false
  Block:
    type: "object"
    properties:
      actions:
        type: "array"
        required: false
        items:
          type: "string"
          enum:
          - "add"
          - "delete"
      blocked:
        type: "boolean"
        required: false
      created:
        type: "Date"
        required: false
      author:
        type: "User"
        required: false
      reason_id:
        type: "integer"
        required: false
      reason:
        type: "string"
        required: false
      category:
        type: "string"
        required: false
      types:
        type: "array"
        required: false
        items:
          type: "string"
          enum:
          - "work_order"
          - "company"
          - "client"
          - "location"
          - "project"
          - "service_company"
  ClientRequest:
    type: "object"
    properties:
      id:
        type: "integer"
        required: false
      name:
        type: "string"
        required: false
      source:
        type: "string"
        enum:
        - "portal"
        - "scrape"
        - "email"
        required: false
  Educations:
    type: "object"
    properties:
      metadata:
        type: "ListEnvelope"
        required: false
      results:
        type: "array"
        required: false
        items:
          type: "Education"
  ListEnvelope:
    type: "object"
    properties:
      total:
        type: "integer"
        required: false
      page:
        type: "integer"
        required: false
      pages:
        type: "integer"
        required: false
      per_page:
        type: "integer"
        required: false
      columns:
        type: "string"
        required: false
      available_columns:
        type: "array"
        required: false
        items:
          type: "AvailableColumn"
      list:
        type: "string"
        required: false
      view:
        type: "string"
        enum:
        - "map"
        - "list"
        - "schedule"
        - "model"
        - "card"
        required: false
      sort:
        type: "string"
        required: false
      order:
        type: "string"
        enum:
        - "asc"
        - "desc"
        required: false
      timings:
        type: "array"
        required: false
        items:
          type: "object"
          properties:
            name:
              type: "string"
              required: false
            time:
              type: "integer"
              required: false
  Pay:
    type: "object"
    properties:
      work_order_id:
        type: "integer"
        required: false
      type:
        type: "string"
        enum:
        - ""
        - "hourly"
        - "fixed"
        - "device"
        - "blended"
        required: false
      correlation_id:
        type: "string"
        required: false
      role:
        type: "string"
        required: false
      minimum_payment:
        type: "number"
        required: false
      actions:
        type: "array"
        required: false
        items:
          type: "string"
          enum:
          - "edit"
          - "print"
          - "view"
      base:
        type: "object"
        required: false
        properties:
          units:
            type: "number"
            required: false
          amount:
            type: "number"
            required: false
      additional:
        type: "object"
        required: false
        properties:
          units:
            type: "number"
            required: false
          amount:
            type: "number"
            required: false
      finance:
        type: "object"
        required: false
        properties:
          id:
            type: "integer"
            required: false
          terms:
            type: "string"
            required: false
          description:
            type: "string"
            required: false
          limit:
            type: "number"
            required: false
      range:
        type: "object"
        required: false
        properties:
          min:
            type: "number"
            required: false
          max:
            type: "number"
            required: false
      pricing_insights:
        type: "PricingInsights"
        required: false
      reported_hours:
        type: "number"
        required: false
      number_of_devices:
        type: "number"
        required: false
      labor_sum:
        type: "number"
        required: false
      total:
        type: "number"
        required: false
      estimated_payment_date:
        type: "Date"
        required: false
      hold:
        type: "PayModifier"
        required: false
      payment:
        type: "PayModifier"
        required: false
      fees:
        type: "array"
        required: false
        items:
          type: "PayModifier"
      bonuses:
        type: "PayModifiers"
        required: false
      penalties:
        type: "PayModifiers"
        required: false
      discounts:
        type: "PayModifiers"
        required: false
      expenses:
        type: "Expenses"
        required: false
      increases:
        type: "PayIncreases"
        required: false
      max_pay:
        type: "number"
        required: false
      term:
        type: "object"
        required: false
        properties:
          id:
            type: "string"
            required: false
          explanation:
            type: "string"
            required: false
  PricingInsights:
    type: "object"
    properties:
      region:
        type: "object"
        required: false
        properties:
          name:
            type: "string"
            required: false
          id:
            type: "integer"
            required: false
          distance:
            type: "number"
            required: false
          providers:
            type: "object"
            required: false
            properties:
              marketplace:
                type: "integer"
                required: false
          average_rate:
            type: "object"
            required: false
            properties:
              first_quartile:
                type: "number"
                required: false
              median:
                type: "number"
                required: false
              third_quartile:
                type: "number"
                required: false
  ProviderRatings:
    type: "object"
    properties:
      ratings:
        type: "integer"
        required: false
      stars:
        type: "number"
        required: false
      categories:
        type: "array"
        required: false
        items:
          type: "object"
          properties:
            type:
              type: "string"
              required: false
            percent:
              type: "integer"
              required: false
  Requests:
    type: "object"
    properties:
      metadata:
        type: "ListEnvelope"
        required: false
      actions:
        type: "array"
        required: false
        items:
          type: "string"
          enum:
          - "add"
          - "counter_offer"
      open_request:
        type: "Request"
        required: false
      results:
        type: "array"
        required: false
        items:
          type: "Request"
  StoredLocation:
    type: "object"
    properties:
      id:
        type: "integer"
        required: false
      name:
        type: "string"
        required: false
      company:
        type: "Company"
        required: false
      group:
        type: "LocationGroup"
        required: false
      type:
        type: "LocationType"
        required: false
      address1:
        type: "string"
        required: false
      address2:
        type: "string"
        required: false
      city:
        type: "string"
        required: false
      state:
        type: "string"
        required: false
      zip:
        type: "string"
        required: false
      country:
        type: "string"
        required: false
      time_zone:
        type: "TimeZone"
        required: false
      contact:
        type: "Contact"
        required: false
      notes:
        type: "LocationNotes"
        required: false
      active:
        type: "boolean"
        required: false
      client:
        type: "Company"
        required: false
      geo:
        type: "Coords"
        required: false
      attributes:
        type: "array"
        required: false
        items:
          type: "LocationAttribute"
      actions:
        type: "array"
        required: false
        items:
          type: "string"
          enum:
          - "edit"
  Schedule:
    type: "object"
    properties:
      work_order_id:
        type: "integer"
        required: false
      correlation_id:
        type: "string"
        required: false
      today_tomorrow:
        type: "string"
        enum:
        - "Today"
        - "Tomorrow"
        - ""
        required: false
      service_window:
        type: "object"
        required: false
        properties:
          start:
            type: "Date"
            required: false
          end:
            type: "Date"
            required: false
          mode:
            type: "string"
            enum:
            - "hours"
            - "between"
            - "exact"
            required: false
      on_my_way:
        type: "OnMyWay"
        required: false
      time_zone:
        type: "TimeZone"
        required: false
      role:
        type: "string"
        required: false
      status_id:
        type: "integer"
        required: false
      actions:
        type: "array"
        required: false
        items:
          type: "string"
          enum:
          - "edit"
      no_refresh:
        type: "boolean"
        required: false
  Shipment:
    type: "object"
    properties:
      id:
        type: "integer"
        required: false
      user:
        type: "User"
        required: false
      name:
        type: "string"
        required: false
      direction:
        type: "string"
        enum:
        - "to site"
        - "from site"
        required: false
      carrier:
        type: "object"
        required: false
        properties:
          name:
            type: "string"
            enum:
            - "fedex"
            - "ups"
            - "usps"
            - "other"
            required: false
          other:
            type: "string"
            required: false
          tracking:
            type: "string"
            required: false
          arrival:
            type: "Date"
            required: false
          arrived:
            type: "Date"
            required: false
      created:
        type: "Date"
        required: false
      task:
        type: "object"
        required: false
        properties:
          id:
            type: "integer"
            required: false
      status:
        type: "string"
        enum:
        - "Arrived"
        - "En Route"
        - "Error"
        - "Lost"
        - "New"
        required: false
      actions:
        type: "array"
        required: false
        items:
          type: "string"
          enum:
          - "edit"
          - "delete"
  Shipments:
    type: "object"
    properties:
      metadata:
        type: "ListEnvelope"
        required: false
      results:
        type: "array"
        required: false
        items:
          type: "Shipment"
      actions:
        type: "array"
        required: false
        items:
          type: "string"
          enum:
          - "add"
  SelectionRule:
    type: "object"
    properties:
      id:
        type: "integer"
        required: false
      name:
        type: "string"
        required: false
      metadata:
        type: "ListEnvelope"
        required: false
      sum:
        type: "object"
        required: false
        properties:
          match:
            type: "integer"
            required: false
          no_match_optional:
            type: "integer"
            required: false
          no_match_required:
            type: "integer"
            required: false
          total:
            type: "integer"
            required: false
      status:
        type: "string"
        enum:
        - "no_match_optional"
        - "no_match_required"
        - "match"
        required: false
      actions:
        type: "array"
        required: false
        items:
          type: "string"
          enum:
          - "delete"
      results:
        type: "array"
        required: false
        items:
          type: "SelectionRuleCriteria"
  ServiceContract:
    type: "object"
    properties:
      id:
        type: "integer"
        required: false
      name:
        type: "string"
        required: false
  Swaps:
    type: "object"
    properties:
      actions:
        type: "array"
        required: false
        items:
          type: "string"
          enum:
          - "add"
      metadata:
        type: "ListEnvelope"
        required: false
      results:
        type: "array"
        required: false
        items:
          type: "Swap"
      correlation_id:
        type: "string"
        required: false
  Swap:
    type: "object"
    properties:
      actions:
        type: "array"
        required: false
        items:
          type: "string"
          enum:
          - "accept"
          - "deny"
          - "delete"
      id:
        type: "integer"
        required: false
      description:
        type: "string"
        required: false
      status:
        type: "string"
        enum:
        - "accepted"
        - "denied"
        - "pending"
        required: false
      from:
        type: "User"
        required: false
      to:
        type: "User"
        required: false
      requestor:
        type: "User"
        required: false
      actor:
        type: "User"
        required: false
      created:
        type: "Date"
        required: false
      accepted:
        type: "Date"
        required: false
      denied:
        type: "Date"
        required: false
  ServiceContracts:
    type: "array"
    items:
      type: "object"
      properties:
        value:
          type: "integer"
          required: false
        label:
          type: "string"
          required: false
  Tags:
    type: "object"
    properties:
      metadata:
        type: "ListEnvelope"
        required: false
      results:
        type: "array"
        required: false
        items:
          type: "Tag"
      correlation_id:
        type: "string"
        required: false
      actions:
        type: "array"
        required: false
        items:
          type: "string"
          enum:
          - "add"
  Tag:
    type: "object"
    properties:
      id:
        type: "integer"
        required: false
      author:
        type: "User"
        required: false
      created:
        type: "Date"
        required: false
      label:
        type: "string"
        required: false
      hex_color:
        type: "string"
        required: false
      types:
        type: "array"
        required: false
        items:
          type: "string"
          enum:
          - "substatus"
          - "hold"
          - "problem"
          - "show_in_dashboard"
          - "hide_from_provider"
          - "custom_company"
          - "provider_can_edit"
      actions:
        type: "array"
        required: false
        items:
          type: "string"
          enum:
          - "delete"
          - "edit"
      valid_statuses:
        type: "array"
        required: false
        items:
          type: "string"
          enum:
          - "created"
          - "published"
          - "routed"
          - "assigned"
          - "workdone"
          - "approved"
          - "paid"
          - "canceled"
  Template:
    type: "object"
    properties:
      id:
        type: "integer"
        required: false
      name:
        type: "string"
        required: false
  TypeOfWork:
    type: "object"
    properties:
      id:
        type: "integer"
        required: false
      industry:
        type: "string"
        required: false
      name:
        type: "string"
        required: false
  Team:
    type: "object"
    properties:
      id:
        type: "integer"
        required: false
      name:
        type: "string"
        required: false
  WorkflowCompletion:
    type: "object"
    properties:
      metadata:
        type: "ListEnvelope"
        required: false
      results:
        type: "array"
        required: false
        items:
          type: "string"
      status:
        type: "string"
        enum:
        - "invalid"
        - "valid"
        required: false
  WorkOrderOverviewValues:
    type: "object"
    properties:
      actions:
        type: "array"
        required: false
        items:
          type: "string"
          enum:
          - "edit"
          - "service_contracts"
          - "networks"
          - "teams"
      team_id:
        type: "integer"
        required: false
      client_id:
        type: "integer"
        required: false
      project_id:
        type: "integer"
        required: false
      service_contract_id:
        type: "integer"
        required: false
      type_of_work_id:
        type: "integer"
        required: false
      network_id:
        type: "array"
        required: false
        items:
          type: "integer"
      clients:
        type: "array"
        required: false
        items:
          type: "object"
          properties:
            value:
              type: "integer"
              required: false
            label:
              type: "string"
              required: false
      projects:
        type: "array"
        required: false
        items:
          type: "object"
          properties:
            value:
              type: "integer"
              required: false
            label:
              type: "string"
              required: false
      networks:
        type: "array"
        required: false
        items:
          type: "object"
          properties:
            value:
              type: "integer"
              required: false
            label:
              type: "string"
              required: false
      service_contracts:
        type: "array"
        required: false
        items:
          type: "object"
          properties:
            value:
              type: "integer"
              required: false
            label:
              type: "string"
              required: false
      types_of_work:
        type: "array"
        required: false
        items:
          type: "object"
          properties:
            value:
              type: "integer"
              required: false
            label:
              type: "string"
              required: false
      teams:
        type: "array"
        required: false
        items:
          type: "object"
          properties:
            value:
              type: "integer"
              required: false
            label:
              type: "string"
              required: false
  WorkOrderOverview:
    type: "object"
    properties:
      type_of_work:
        type: "object"
        required: false
        properties:
          company:
            type: "object"
            required: false
            properties:
              jobs:
                type: "integer"
                required: false
              average_hourly_rate:
                type: "number"
                required: false
              average_duration:
                type: "number"
                required: false
          marketplace:
            type: "object"
            required: false
            properties:
              average_hourly_rate:
                type: "number"
                required: false
              average_duration:
                type: "number"
                required: false
      company:
        type: "object"
        required: false
        properties:
          average_duration:
            type: "integer"
            required: false
          jobs:
            type: "integer"
            required: false
          paid:
            type: "number"
            required: false
          percent_counters:
            type: "number"
            required: false
          percent_expenses:
            type: "number"
            required: false
          active_jobs:
            type: "integer"
            required: false
          cancelled_jobs:
            type: "integer"
            required: false
  WorkOrderRatings:
    type: "object"
    properties:
      correlation_id:
        type: "string"
        required: false
      buyer:
        type: "object"
        required: false
        properties:
          actions:
            type: "array"
            required: false
            items:
              type: "string"
              enum:
              - "add"
              - "edit"
          manager:
            type: "User"
            required: false
          company:
            type: "object"
            required: false
            properties:
              id:
                type: "integer"
                required: false
          overall:
            type: "object"
            required: false
            properties:
              ratings:
                type: "integer"
                required: false
              stars:
                type: "number"
                required: false
              jobs:
                type: "integer"
                required: false
              percent_clear_expectations:
                type: "integer"
                required: false
              percent_respectful:
                type: "integer"
                required: false
              approval_days:
                type: "integer"
                required: false
              approval_period:
                type: "integer"
                required: false
              percent_approval:
                type: "array"
                required: false
                items:
                  type: "object"
                  properties:
                    days:
                      type: "integer"
                      required: false
                    percent:
                      type: "integer"
                      required: false
          work_order:
            type: "object"
            required: false
            properties:
              created:
                type: "Date"
                required: false
              stars:
                type: "integer"
                required: false
              recommend:
                type: "boolean"
                required: false
              comment:
                type: "string"
                required: false
              hidden:
                type: "boolean"
                required: false
              flagged:
                type: "boolean"
                required: false
              form:
                type: "string"
                enum:
                - "old"
                - "new"
                required: false
              categories:
                type: "array"
                required: false
                items:
                  type: "object"
                  properties:
                    type:
                      type: "string"
                      enum:
                      - "professionalism"
                      - "scope"
                      - "respect"
                      required: false
                    rating:
                      type: "integer"
                      required: false
