#%RAML 1.0 Library
types:
  AnimalWithAddress:
    type: object
    additionalProperties: true
    properties:
      address:
        required: true
        type: string
  Cat:
    type: object
    additionalProperties: true
    properties:
      address:
        required: true
        type: string
      age:
        required: true
        type: union
        anyOf:
          -
            type: integer
          -
            type: number
