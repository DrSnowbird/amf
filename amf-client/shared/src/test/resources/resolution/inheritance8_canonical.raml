#%RAML 1.0 Library
types:
  CatWithCity:
    type: object
    additionalProperties: true
    properties:
      city:
        required: true
        type: string
  CatWithAddress:
    type: object
    additionalProperties: true
    properties:
      address:
        required: true
        type: string
  Cat:
    type: union
    anyOf:
      -
        type: object
        additionalProperties: true
        properties:
          city:
            required: true
            type: string
          address:
            required: true
            type: string
          age:
            required: true
            type: string
      -
        type: object
        additionalProperties: true
        properties:
          city:
            required: true
            type: string
          address:
            required: true
            type: string
          age:
            required: true
            type: number
