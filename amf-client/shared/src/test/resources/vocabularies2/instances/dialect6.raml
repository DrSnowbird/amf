#%RAML 1.0 Dialect
dialect: Test
version: 1.0
nodeMappings:
  A:
    classTerm: tmp.A
    mapping:
      a2:
        propertyTerm: tmp.a2
        range: string
      a1:
        propertyTerm: tmp.a1
        range: string
  RootNode:
    classTerm: tmp.Test
    mapping:
      a:
        propertyTerm: tmp.a
        range: A
        mapKey: tmp.a1
documents:
  root:
    encodes: RootNode
    declares:
      As: A
external:
  tmp: http://test.com/tmp#